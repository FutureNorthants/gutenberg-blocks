{
  "name": "future-northants-blocks",
  "version": "0.1.0",
  "description": "A collection of Gutenberg Blocks for the Future Northants WordPress starter theme",
  "author": "The WordPress Contributors",
  "license": "GPL-3.0-or-later",
  "main": "build/index.js",
  "scripts": {
    "build": "npm-run-all build:section-link build:section-links",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip",
    "start:section-link": "wp-scripts start blocks/section-link/src/index.js --output-path=blocks/section-link/build/",
    "build:section-link": "wp-scripts build blocks/section-link/src/index.js --output-path=blocks/section-link/build/",
    "start:section-links": "wp-scripts start blocks/section-links/src/index.js --output-path=blocks/section-links/build/",
    "build:section-links": "wp-scripts build blocks/section-links/src/index.js --output-path=blocks/section-links/build/"
  },
  "devDependencies": {
    "@wordpress/scripts": "^24.6.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@wordpress/components": "^22.1.0"
  }
}
